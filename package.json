{
  "name": "fastify-wasm",
  "version": "1.0.0",
  "description": "Plugin for loading and executing WebAssembly modules in Fastify.",
  "main": "index.js",
  "type": "commonjs",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "npm run test:unit && npm run test:typescript",
    "test:typescript": "tsd",
    "test:unit": "c8 --100 node --test",
    "test:unit:report": "c8 -r html --100 node --test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dueen/fastify-wasm.git"
  },
  "keywords": [
    "fastify",
    "wasm",
    "webassembly",
    "plugin"
  ],
  "author": "Dueen Eduarda - @Dueen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dueen/fastify-wasm/issues"
  },
  "homepage": "https://github.com/dueen/fastify-wasm#readme",
  "dependencies": {
    "fastify-plugin": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.17",
    "c8": "^10.1.3",
    "eslint": "^9.22.0",
    "fastify": "^5.2.2",
    "neostandard": "^0.12.1",
    "tsd": "^0.32.0"
  },
  "tsd": {
    "directory": "test/types"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.1.0"
  }
}
